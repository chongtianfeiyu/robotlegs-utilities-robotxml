package org.robotlegs.utilities.robotxml.commands {	import org.robotlegs.utilities.robotxml.models.Injections;	import org.robotlegs.utilities.robotxml.models.Location;		import org.robotlegs.mvcs.Command;		import flash.filesystem.File;		public class WriteXMLCommand extends Command	{				[inject] public var injections:Injections;		[Inject] public var locations:Location;				public function WriteXMLCommand() 		{		}				override public function execute():void 		{						var output:String = "<types>"						// get the location for the "xmlfile"			var file:File = new File(locations.filePath);						// get the data from the model			var injectData:Array = injections.getInjections();						var item:Object;			var names:Array;						var count:Number = injectData.length;			while(count--)			{				item = injectData[count];				output += "<type name=\"" + item.fqdn + "\">";				while(item.names.length)				{					output += "<field name=\"" + item.names.pop() + "\" />";				}				output += "</type>";			}			output += "</types>";						// convert it into xml			var outputXML:XML = new XML(output);			trace(outputXML);			// write the data to the file			file.save(outputXML, file.name);						// close the file. 						// DONE		} 	}}