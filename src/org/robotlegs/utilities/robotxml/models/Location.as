package org.robotlegs.utilities.robotxml.models {	import org.robotlegs.utilities.robotxml.events.LocationEvent;		import org.robotlegs.mvcs.Actor;		public class Location extends Actor	{		private var _directoryPath:String;				public function Location() 		{		}						public function set folderPath(folder:String):void		{			trace("Location.folderPath = " + folder);			_directoryPath = folder;			dispatch(new LocationEvent(LocationEvent.NEW_FOLDER));		}		public function get folderPath():String		{			return _directoryPath;		}					}}