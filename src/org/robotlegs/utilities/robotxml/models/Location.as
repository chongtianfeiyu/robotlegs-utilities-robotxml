package org.robotlegs.utilities.robotxml.models {	import org.robotlegs.utilities.robotxml.events.LocationEvent;		import org.robotlegs.mvcs.Actor;		public class Location extends Actor	{		private var _directoryPath:String;		private var _filePath:String;				public function Location() 		{		}						public function set folderPath(folder:String):void		{			trace("Location.folderPath = " + folder);			_directoryPath = folder;			dispatch(new LocationEvent(LocationEvent.NEW_FOLDER));		}		public function get folderPath():String		{			return _directoryPath;		}				public function set filePath(file:String):void		{			trace("Location.filePath = " + file);			_filePath = file;			dispatch(new LocationEvent(LocationEvent.NEW_FILE));		}		public function get filePath():String		{			return _filePath;		}			}}