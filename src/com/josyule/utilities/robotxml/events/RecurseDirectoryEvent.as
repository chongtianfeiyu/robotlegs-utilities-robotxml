package com.josyule.utilities.robotxml.events {		import flash.events.Event;	import flash.filesystem.File;		public class RecurseDirectoryEvent extends Event {				public static const FILE:String = "rde_file";				private var _file:File;				public function RecurseDirectoryEvent(name:String, aFile:File) {			_file = aFile;			super(name);		} 				public function get theFile():File		{			return _file;		}					}}