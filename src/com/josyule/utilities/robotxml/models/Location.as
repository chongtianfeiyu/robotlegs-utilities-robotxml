package com.josyule.utilities.robotxml.models {		import org.robotlegs.mvcs.Actor;		public class Location extends Actor	{		private var _directoryPath:String;		private var _filePath:String;				public function Location() 		{		}						public function set folderPath(folder:String):void		{			trace("Location.folderPath = " + folder);			_directoryPath = folder;		}		public function get folderPath():String		{			return _directoryPath;		}				public function set filePath(file:String):void		{			trace("Location.filePath = " + file);			_filePath = file;		}		public function get filePath():String		{			return _filePath;		}			}}